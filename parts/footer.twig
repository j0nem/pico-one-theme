<footer itemscope itemtype="http://schema.org/WPFooter">

    <div class="footer-text">
        {{ meta.footer.text }}
    </div>
    <nav class="footer" itemscope itemtype="http://schema.org/SiteNavigationElement">
        {% for elem in meta.footer.links %}
            <a href="{{ elem.link }}">{{ elem.label }}</a>
        {% endfor %}
    </nav>

</footer>